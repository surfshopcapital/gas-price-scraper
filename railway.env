# Railway Environment Configuration
NONINTERACTIVE=1
CHROME_BIN=/nix/var/nix/profiles/default/bin/chromium
CHROMEDRIVER=/nix/var/nix/profiles/default/bin/chromedriver

# Chrome environment variables
CHROME_DEVEL_SANDBOX=/nix/var/nix/profiles/default/bin/chromium-sandbox
CHROME_EXTRA_ARGS="--no-sandbox --disable-dev-shm-usage"

# Python environment
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
